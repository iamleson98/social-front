		"test:integration": "playwright test",
		"test:unit": "vitest",
		"typesafe-i18n": "typesafe-i18n",
		"model-gen": "dotenv -e .env.local -- typeorm-model-generator -h localhost -d saleor -u saleor -x saleor -e postgres -o ./src/lib/db/models",
